{
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": "airbnb",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "import/extensions": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-extraneous-unresolved": 0,
      "import/no-unresolved": 0,
      "no-param-reassign": 1,
      "react/prefer-stateless-function": 0
    }
}
